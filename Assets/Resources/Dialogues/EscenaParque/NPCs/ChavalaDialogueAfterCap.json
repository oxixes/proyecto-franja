            "imageAssetId": "IMGChavala"
        },
        {
            "type": "text",
            "characterName": "Chat",
            "text": "PERMA PERMA PERMA.",
            "pause": true,
            "imageAssetId": null
        },
        {
            "type": "text",
            "characterName": null,
            "text": "<i>La chavala se da la vuelta y te mira.</i>",
            "pause": true,
            "imageAssetId": null
        },
        {
            "type": "text",
            "characterName": "Chavala",
            "text": "Quedas permabaneado de mi grupo.",
            "pause": true,
            "imageAssetId": "IMGChavala"
        },
        {
            "type": "text",
            "characterName": null,
            "text": "<i>Te suelta una ostia</i>",
